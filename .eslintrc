{
  "parser": "babel-eslint",
  "env": {
    browser: true,
    node: true
  },
  "plugins": [
    "react"
  ],
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "comma-dangle": 0,
    "no-console": 0
  },
  "globals": {
    "Promise": false
  }
}
